local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Starving Artists Copy Art -- By Retro", "GrapeTheme")

local Tab = Window:NewTab("Starving Artists Script")
local Section = Tab:NewSection("Script")
local Section = Section:NewButton("Press To Execute Script", "PRESS TO EXCUTE", function ()
    local Library = loadstring(game:HttpGet("https://pastebin.com/raw/aCFfd9Uq"))()
end)


local Tab = Window:NewTab("JOIN MY DISCORD")
local Section = Tab:NewSection("Discord Invite")
local Section = Section:NewButton("Press To Copy Invite To Clipboard", "Please Join :D", function ()
    setclipboard('https://discord.gg/Hfy2KXvu')
end)

